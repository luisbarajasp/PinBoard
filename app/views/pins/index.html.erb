<div class="skinny_wrapper wrapper_padding">
    <div class="row">
        <% @pins.each do |pin| %>
        <div class="col-md-4">
            <div class="panel panel-default">
                <div class="panel-heading">
                    <h3 class="panel-title"><%= pin.title %></h3>
                 </div>
                  <div class="panel-body">
                    <%= link_to image_tag(pin.image.url(:medium)), pin %>
                    <p><%= pin.description %></p>
                  </div>
            </div>
        </div>
        <% end %>
    </div>
</div>
